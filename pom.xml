    <groupId>com.indiv</groupId>
    <!--功能模块名-->
    <artifactId>javabasis</artifactId>
    <!--项目的版本-->
    <version>1.0.0-SNAPSHOT</version>
    <dependencies>
        <dependency>
            <groupId>cglib</groupId>
            <artifactId>cglib-nodep</artifactId>
            <version>3.3.0</version>
            <scope>test</scope>
        </dependency>
    </dependencies>
</project>
